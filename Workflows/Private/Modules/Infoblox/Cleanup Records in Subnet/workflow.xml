  <workflow-item name="item2" out-name="item3" type="task">
    <display-name><![CDATA[Discover Records]]></display-name>
    <script encoded="false"><![CDATA[var obj = JSON.parse(contentAsString);
//System.log(obj[0].types[0]);

var usedIps = [];

for(o in obj) {
	if(obj[o].status == "USED") {
		if(obj[o].types[0] !== "NETWORK" && obj[o].types[0] !== "BROADCAST" ){
			usedIps.push(obj[o]);
		}
	}
}

System.log("There are "+usedIps.length+" used in the "+network+" network.");
for(u in usedIps) {
	System.log(usedIps[u].ip_address);
}
	]]></script>
    <in-binding>
