    <display-name><![CDATA[Parse JSON]]></display-name>
    <script encoded="false"><![CDATA[// Parse the JSON payload
var obj = JSON.parse(contentAsString);

// Get values from Payload
var network = obj[0].network;
var netView = obj[0].network_view;
System.log(network);
System.log(netView);

// Set networkView if a network view was discovered in the JSON payload, otherwise use default.
if(netView) {
	networkView = netView;
}

// Return network and CIDR from network variable
var slash = network.search("/");
var netaddr = network.slice(0,slash);
var cidr = network.slice(slash+1,network.length);
var networkAddress = netaddr+"/"+cidr;
System.log("Network: "+netaddr);
System.log("CIDR: "+cidr);
System.log("Full Network Address: "+networkAddress);]]></script>
