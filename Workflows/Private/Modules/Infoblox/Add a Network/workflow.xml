    <display-name><![CDATA[Set variables]]></display-name>
    <script encoded="false"><![CDATA[// Set base network based on location selected
var network;
var longlocation;
if(location == "frk") {
	network = "10.38.0.0/16";
	longlocation = "Franklin";
} else {
	network = "10.40.0.0/16";
	longlocation = "Milwaukee";
}
var comment = bg+" "+longlocation;


// Form content payload to be sent 
var content = '{"network": "func:nextavailablenetwork:'+network+',default,'+cidr+'", "network_view":"default", "extattrs": {"Built By":{"value":"Unknown"}}, "comment":"'+comment+'"}']]></script>
