  <workflow-item name="item1" out-name="item0" throw-bind-name="errorCode" type="task">
    <display-name><![CDATA[Scriptable task]]></display-name>
    <script encoded="false"><![CDATA[//Getting All Business Groups
busGroups = vCACCAFEEntitiesFinder.getSubtenants(host)
var pdls = [];
busGroups.sort();

//Looping through each to get nm.ucp.businessGroupPDL
for each (var busGroup in busGroups) {
System.log("Business Group Name = "+busGroup.name);
var pdl = vCACCAFESubtenantHelper.getCustomPropertyValue(busGroup,customPropName);
System.log("PDL value = "+pdl);
pdls.push(pdl);

}
System.log(pdls)]]></script>
