  <workflow-item name="item3" out-name="item5" type="task">
    <display-name><![CDATA[Define Variables]]></display-name>
    <script encoded="false"><![CDATA[// Get AD Host from properties defined for the VM.
var vmProps = new Properties;
var vcacVM = System.getModule("com.nm.ucp").getVcacVmFromVCvm(vm);
var host = System.getModule("com.nm.ucp").getVcacHost("vcacHost");
vmProps = System.getModule("com.vmware.library.vcac").getPropertiesFromVirtualMachine(host, vcacVM)

var environment = vmProps.get("ucp.tag.environment");
var adHostString;

//Define ADHostString
if(environment == "Production") {
	adHostString = "adProd";
} else if(environment == "Non-Production") {
	adHostString = "adNonProd";
} else {
	adHostString = "adDev";
}

//Get ADHost
var adHost = System.getModule("com.nm.ucp").getADHost(adHostString);
System.log("adHost: "+adHost.name);

var vmName = vcacVM.virtualMachineName;]]></script>
    <in-binding>
      <bind name="vm" type="VC:VirtualMachine" export-name="vm"/>
    </in-binding>
    <out-binding>
      <bind name="adHost" type="AD:AdHost" export-name="adHost"/>
      <bind name="vmName" type="string" export-name="vmName"/>
    </out-binding>
