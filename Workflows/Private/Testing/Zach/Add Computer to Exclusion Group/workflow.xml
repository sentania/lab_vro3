  <workflow-item name="item1" out-name="item3" type="task">
    <display-name><![CDATA[Define Variables]]></display-name>
    <script encoded="false"><![CDATA[// From Server's FQDN, determine domain and then userGroup.
var domain;
if(DNS.search("nmdev.nmfco.com") > -1) {
	userGroup = devDomainGroup;
	domain = "nmdev.nmfco.com";
	adHostString = "adDev";
} else if(DNS.search("nmtest.nmfco.com") > -1) {
	userGroup = testDomainGroup;
	domain = "nmtest.nmfco.com";
	adHostString = "adNonProd";
} else {
	userGroup = baseDomainGroup;
	domain = "nm.nmfco.com";
	adHostString = "adProd";
}

var computerName = vm.name;]]></script>
    <in-binding>
      <bind name="vm" type="VC:VirtualMachine" export-name="vm"/>
      <bind name="baseDomainGroup" type="AD:UserGroup"/>
      <bind name="devDomainGroup" type="AD:UserGroup"/>
      <bind name="testDomainGroup" type="AD:UserGroup"/>
      <bind name="DNS" type="string" export-name="DNS"/>
    </in-binding>
    <out-binding>
      <bind name="userGroup" type="AD:UserGroup" export-name="userGroup"/>
      <bind name="adHostString" type="string" export-name="adHostString"/>
      <bind name="computerName" type="string" export-name="computerName"/>
    </out-binding>
